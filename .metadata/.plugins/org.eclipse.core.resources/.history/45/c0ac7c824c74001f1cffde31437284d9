package um;

public class Circulo implements FormaGeometrica{
	private double raio=0;
	
	@Override
	public void area() {
		
		System.out.println("O Área do círculo é "+((raio*raio)*3.1415));
	}

	@Override
	public void comprimento() {
		
		System.out.println("O Comprimento do círculo é "+(2*(raio*3.1415)));
	}

	public double getRaio() {
		return raio;
	}

	public void setRaio(double raio) {
		this.raio = raio;
	}
	
}
